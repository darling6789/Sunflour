  transition: all 0.2s;
  display: inline-flex;
  align-items: center;
  gap: 6px;
  margin: 0 0 0 54px;
  width: fit-content;
}

.s-m-diamond-picker-modal__hide-filters-btn:hover {
  background: rgba(255, 255, 255, 0.70);
  border-color: rgba(96, 77, 62, 0.70);
}

.s-m-diamond-picker-modal__hide-filters-btn svg {
  width: 20px;
  height: 20px;
  flex-shrink: 0;
}

 .s-m-diamond-picker-modal__filters.hidden {
