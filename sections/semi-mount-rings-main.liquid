              {%- when '@app' -%}
                {% render block %}
              {%- when 'guarantee' -%}
                <div class="guarantees-wrap s-m-guarantees-wrap">
                  <div
                    id="guarantee-{{ section.id }}"
                    role="status"
                    {{ block.shopify_attributes }}
                    class="guarantee-badges s-m-guarantee-badges"
