                    <span>DISCLAIMER</span>
                    Marc Robinson Jewelers is not an authorized dealer, distributor, or affiliated in any way with Rolex
                    S.A., Rolex USA, or any of its subsidiaries. Rolex is a registered trademark of Rolex S.A. Any
                    trademarks, brand names, or logos mentioned are for identification purposes only and remain the
                    property of their respective owners. All Rolex watches sold by Marc Robinson Jewelers are pre-owned,
                    authentic Rolex timepieces. Rolex has no obligation to service watches sold by Marc Robinson
                    Jewelers. For Rolexs official terms and services, please visit an authorized Rolex dealer or
                    contact Rolex directly.
                  </p>
                  <div class="disclaimer-badge__image">
                    <img src="{{ 'icon-info.svg' | asset_url }}" alt="Disclaimer Badge">
                  </div>
                </div>
              {% when 'trust_badge' %}
                <div class="trust-badges">
                  <div class="trust-badge">
                    <div class="trust-badge__image">
                      <img src="{{ block.settings.image1 | img_url: 'master' }}" alt="Trust Badge">
                    </div>
                    <div class="trust-badge__content">
                      <h3>Original Warranty</h3>
                      <p>Retain the manufacturers warranty coverage.</p>
                    </div>
                  </div>
                  <div class="trust-badge">
                    <div class="trust-badge__image">
                      <img src="{{ block.settings.image2 | img_url: 'master' }}" alt="Trust Badge">
                    </div>
                    <div class="trust-badge__content">
                      <h3>Fast Insured Shipping</h3>
                      <p>Rest easy with insured shipping! Shipping costs are calculated at checkout.</p>
                    </div>
                  </div>
                  <div class="trust-badge">
                    <div class="trust-badge__image">
                      <img src="{{ block.settings.image3 | img_url: 'master' }}" alt="Trust Badge">
                    </div>
                    <div class="trust-badge__content">
                      <h3>Easy Returns</h3>
                      <p>7 day return window for online purchases.</p>
                    </div>
                  </div>
                </div>
              {% when 'consult_an_expert' %}
                <div class="consult-an-expert">
                  <a href="{{ block.settings.consult_link }}">
                    <h3 style="font-style: italic;">Consult an Expert</h3>
                  </a>
                </div>
              {% when 'rolex_watch_metafields' %}
                <div class="rolex-watch-metafields">
                  <div class="rolex-watch-metafields__band-size">
                    {{ product.metafields.custom.watch_band_type }} | {{ product.metafields.custom.watch_case_size }}
                  </div>
                  <div class="rolex-watch-metafields__content">
                    <div class="rolex-watch-metafields__content-item-wrapper">
                      <div class="rolex-watch-metafields__content-item">
                        <div class="rolex-watch-metafields__content-item-label">MODEL</div>
                        <div class="rolex-watch-metafields__content-item-value">
                          {{ product.metafields.custom.watch_reference }}
                        </div>
                      </div>
                      <div class="rolex-watch-metafields__content-item">
                        <div class="rolex-watch-metafields__content-item-label">YEAR</div>
                        <div class="rolex-watch-metafields__content-item-value">
                          {{ product.metafields.custom.watch_year }}
                        </div>
                      </div>
                      <div class="rolex-watch-metafields__content-item">
                        <div class="rolex-watch-metafields__content-item-label">CONDITION</div>
                        <div class="rolex-watch-metafields__content-item-value">
                          {% assign raw_condition = product.metafields.custom.watch_condition | downcase | strip %}
                          {% case raw_condition %}
                            {% when 'unworn' -%}
                              UNWORN
                            {% when 'unworn (nos)' -%}
                              UNWORN
                            {% when 'as is' -%}
                              RETAIL READY
                            {% when 'slider' -%}
                              EXCELLENT
                            {% when 'mint' -%}
                              VERY GOOD
                            {% when 'very good' -%}
                              VERY GOOD
                            {% when 'pre-owned' -%}
                              GOOD
                            {% when 'good' -%}
                              GOOD
                            {% else -%}
                              {{- product.metafields.custom.watch_condition | upcase }}
                          {% endcase %}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
            {%- endcase -%}
          {%- endfor -%}
          <a href="{{ product.url }}" class="link product__view-details animate-arrow">
            {{ 'products.product.view_full_details' | t }}
            {{- 'icon-arrow.svg' | inline_asset_content -}}
          </a>
        </section>
      </div>
    </div>

    {% render 'product-media-modal', variant_images: variant_images %}

    {% assign popups = section.blocks | where: 'type', 'popup' %}
    {%- for block in popups -%}
      <modal-dialog id="PopupModal-{{ block.id }}" class="product-popup-modal" {{ block.shopify_attributes }}>
        <div
          role="dialog"
          aria-label="{{ block.settings.text }}"
          aria-modal="true"
          class="product-popup-modal__content"
          tabindex="-1"
        >
          <button
            id="ModalClose-{{ block.id }}"
            type="button"
            class="product-popup-modal__toggle"
            aria-label="{{ 'accessibility.close' | t }}"
          >
            {{- 'icon-close.svg' | inline_asset_content -}}
          </button>
          <div class="product-popup-modal__content-info">
            <h1 class="h2">{{ block.settings.page.title | escape }}</h1>
            {{ block.settings.page.content }}
          </div>
        </div>
      </modal-dialog>
    {%- endfor -%}

    {%- if product.media.size > 0 -%}
      <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- if first_3d_model -%}
      <script type="application/json" id="ProductJSON-{{ product.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
      <script src="{{ 'product-model.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- liquid
      if product.selected_or_first_available_variant.featured_media
        assign seo_media = product.selected_or_first_available_variant.featured_media
      else
        assign seo_media = product.featured_media
      endif
    -%}

    <script type="application/ld+json">
      {{ product | structured_data }}
    </script>
  </div>
</product-info>
